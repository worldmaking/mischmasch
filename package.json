{
  "name": "msvr",
  "version": "0.0.1",
  "description": "Modular Synthesis in VR",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "test": "jest"
  },
  "jest": {
    "verbose": true,
    "modulePathIgnorePatterns": [
      "got/"
    ],
    "moduleDirectories": [
      "__tests__/deltas/",
      "node_modules"
    ],
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "Mischmasch Test Report",
          "includeFailureMsg": true
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/worldmaking/msvr.git"
  },
  "author": "Alice Lab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/worldmaking/msvr/issues"
  },
  "homepage": "https://github.com/worldmaking/msvr#readme",
  "dependencies": {
    "bottleneck": "^2.19.5",
    "cors": "^2.8.5",
    "cycle-test": "^1.0.0",
    "express": "^4.17.1",
    "find-cycle": "^1.0.0",
    "gl-matrix": "^3.3.0",
    "http": "0.0.1-security",
    "ip": "^1.1.5",
    "jest-html-reporter": "^3.1.3",
    "load-bmfont": "^1.4.0",
    "nodemon": "^1.19.4",
    "progress": "^2.0.3",
    "public-ip": "^4.0.1",
    "reconnecting-websocket": "^4.4.0",
    "simple-peer": "^9.6.2",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "source-map-support": "^0.5.19",
    "tasklist": "^4.0.1",
    "three-bmfont-text": "^2.3.0",
    "username": "^5.1.0",
    "wrtc": "^0.4.4",
    "ws": "^6.2.1",
    "yargs": "^15.3.1"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "ext": "js html"
  },
  "devDependencies": {
    "jest": "^26.0.1"
  }
}
